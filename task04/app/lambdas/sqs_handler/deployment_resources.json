{
  "sqs_handler-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution",
      {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "sqs:ReceiveMessage",
              "sqs:DeleteMessage",
              "sqs:GetQueueAttributes",
              "sns:Subscribe",
              "sns:Publish",
              "sns:GetTopicAttributes",
              "sns:ListTopics"
            ],
            "Resource": "*"
          }
        ]
      }
    ],
    "resource_type": "iam_role",
    "tags": {}
  }
}
